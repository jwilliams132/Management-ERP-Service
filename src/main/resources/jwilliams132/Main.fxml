<BorderPane xmlns="http://javafx.com/javafx/8.0.171"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="jwilliams132.Controller_Main">
	<top>
		<MenuBar fx:id="menuBar">
			<menus>
				<Menu text="File">
					<items>
						<MenuItem text="Open files..." onAction="#openFiles" />
						<MenuItem text="Update Info" onAction="#updateInfo" />
						<MenuItem text="Save Files..." onAction="#saveFiles" />
						<MenuItem text="Export Excel..." onAction="#exportExcel" />
						<SeparatorMenuItem />
						<MenuItem text="Close" />
					</items>
				</Menu>
				<Menu text="Edit"></Menu>
				<Menu text="View"></Menu>
				<Menu text="Reports"></Menu>
				<Menu text="Help">
					<items>
						<MenuItem text="Help" />
					</items>
				</Menu>
			</menus>
		</MenuBar>
	</top>
	<left>
		<!-- <fx:include source="NavBar.fxml" /> -->
	</left>
	<center>
		<fx:include source="Startup.fxml" />
	</center>
